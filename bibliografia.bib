@inproceedings{Soares:2014aa,
	Abstract = {The automatic generation of hypermedia documents is a subject that is still little explored. This paper deals with authoring of dynamic hypermedia documents based on templates. A framework to support the automatic generation of these documents at runtime is proposed and detailed. As an example of use, the framework is instantiated for the automatic development of NCL dynamic applications, also bringing a discussion of the available resources and the native support provided by the Ginga middleware for managing such application types.},
	Acmid = {2664559},
	Address = {New York, NY, USA},
	Annote = {A ideia desse trabalho {\'e} utilizar o processador de templates do TAL para fazer o processamento de
um documento, com um template, on the fly. Fazendo isso d{\'a}-se a ideia de que a aplica{\c c}{\~a}o {\'e} adaptada
automaticamente de acordo com sua execu{\c c}{\~a}o.

O funcionamento se d{\'a} da seguinte forma:
1- Um documento NCL engloba a aplica{\c c}{\~a}o ``adaptativa'' sendo apresentada
2- Algum evento dentro da aplica{\c c}{\~a}o dispara um n{\'o} Lua (Agente 1), possivelmente passando algum
    par{\^a}metro. Esse script Lua vai fazer uma busca em um banco de dados, ou qualquer outra coisa
    e gerar um documento que usa um template, de acordo com as modifica{\c c}{\~o}es que devem ser feitas
    na aplica{\c c}{\~a}o inicial.
3- O documento gerado pelo Agente 1 e o template s{\~a}o passados para o processador de templates
    TAL - em Lua - que gera um documento final.
4- O Filtro, recebe o documento e computa que modifica{\c c}{\~o}es devem ser feitas na aplica{\c c}{\~a}o inicial para
    que ela vire a que acabou de ser processada
5- O filtro emite comandos de edi{\c c}{\~a}o ao vivo (usando a biblioteca Lua de comandos de edi{\c c}{\~a}o)
    modificando assim a aplica{\c c}{\~a}o.

A ideia {\'e} bem interessante! Basicamente o artigo apresenta a mesma coisa que o artigo de 2012 do
DocEng, mas com mais exemplos, ao que parece.},
	Author = {Soares, Luiz Fernando G. and Neto, Carlos de Salles S. and Junior, Jos{\'e} Geraldo de Sousa},
	Booktitle = {Proceedings of the 20th Brazilian Symposium on Multimedia and the Web},
	Date-Added = {2015-08-10 09:13:15 +0000},
	Date-Modified = {2015-11-16 18:58:15 +0000},
	Isbn = {978-1-4503-3230-9},
	Keywords = {Dynamic Hypermedia Applications; NCL; Ginga; Digital TV},
	Location = {Jo{\~a}o Pessoa, Brazil},
	Numpages = {8},
	Pages = {207-214},
	Publisher = {ACM},
	Series = {WebMedia '14},
	Title = {Framework for Automatic Generation of Hypermedia Applications in Runtime},
	Year = {2014},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1145/2664551.2664559}}

@inproceedings{Junior:2012aa,
	Abstract = {This paper describes a NCL 3.0 hypermedia application de- velopment toolchain which supports the verification of tem- poral and spatial consistency of NCL applications. In this toolchain, the application is translated into a TSS code and temporal logic formulas are used for checking the desired properties. It is proposed an approach for incremental veri- fication of live NCL applications. The paper illustrates the proposed approach with a case study of a NCL-based inter- active TV application.},
	Acmid = {2382685},
	Address = {New York, NY, USA},
	Annote = {J{\'u}nior et al. in \cite{Junior:2012aa} use a model-driven approach for the validation of NCL documents. The validation is achieved by transforming an NCL document into a Petri Net. This transformation is done in two steps. In the first step, the document is represented in a language called FIACRE as a set of components and processes (representing the behavior of a component). The second step transforms the FIACRE representation into a Petri Net. The validation uses a model-checking tool and temporal logic formulas representing the properties to be validated. Spatial validation is briefly discussed in \cite{Junior:2012aa} and is performed over the document initial positioning. Their approach covers both temporal and spatial dimensions, however the spatial dimension is static.},
	Author = {J{\'u}nior, Delcino Picinin and Farines, Jean-Marie and Koliver, Cristian},
	Booktitle = {Proceedings of the 18th Brazilian Symposium on Multimedia and the Web},
	Date-Added = {2015-08-10 09:00:52 +0000},
