	Isbn = {978-1-4503-1706-1},
	Keywords = {NCL, incremental verification, interactive digital TV, model checking},
	Location = {S{\~a}o Paulo/SP, Brazil},
	Numpages = {10},
	Pages = {223-232},
	Publisher = {ACM},
	Rating = {3},
	Read = {1},
	Series = {WebMedia '12},
	Title = {An Approach to Verify Live NCL Applications},
	Year = {2012},
